#include <stdio.h>

int main() {
    int arr1[100], arr2[100], n, i, isEqual = 1;

    // Input size
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    // Input first array
    printf("Enter %d elements for first array:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr1[i]);
    }

    // Input second array
    printf("Enter %d elements for second array:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr2[i]);
    }

    // Compare arrays
    for(i = 0; i < n; i++) {
        if(arr1[i] != arr2[i]) {
            isEqual = 0;
            break;
        }
    }

    // Output result
    if(isEqual)
        printf("Both arrays are equal.\n");
    else
        printf("Arrays are not equal.\n");

    return 0;
}
