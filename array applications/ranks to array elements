#include <stdio.h>

int main() {
    int arr[100], rank[100], n, i, j;

    // Input size of array
    printf("Enter number of elements: ");
    scanf("%d", &n);

    // Input array elements
    printf("Enter %d elements:\n", n);
    for(i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    // Assign ranks
    for(i = 0; i < n; i++) {
        rank[i] = 1;  // start with rank 1
        for(j = 0; j < n; j++) {
            if(arr[j] > arr[i]) {
                rank[i]++;
            }
        }
    }

    // Display elements with their ranks
    printf("\nElement\tRank\n");
    for(i = 0; i < n; i++) {
        printf("%d\t%d\n", arr[i], rank[i]);
    }

    return 0;
}
