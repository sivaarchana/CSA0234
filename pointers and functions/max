#include <stdio.h>

// Function to find max using pointers
int findMax(int *a, int *b) {
    if (*a > *b)
        return *a;
    else
        return *b;
}

int main() {
    int x, y, max;

    printf("Enter two numbers: ");
    scanf("%d %d", &x, &y);

    max = findMax(&x, &y);  // Call function with address

    printf("Maximum value is: %d\n", max);

    return 0;
}
