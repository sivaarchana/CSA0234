#include <stdio.h>
#include <string.h>

int main() {
    char str1[100], str2[50];

    printf("Enter first string: ");
    gets(str1);  // Or use fgets(str1, 100, stdin);

    printf("Enter second string: ");
    gets(str2);

    strcat(str1, str2);  // Concatenates str2 to the end of str1

    printf("Concatenated string: %s\n", str1);

    return 0;
}
